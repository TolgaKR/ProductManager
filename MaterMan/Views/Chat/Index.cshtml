
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<MaterMan.Entity.AppUser>

<h2>Kullanıcılar</h2>

<ul>
    @foreach (var user in Model)
    {
        <li>
            <a asp-controller="Chat" asp-action="ChatWith" asp-route-userId="@user.Id">
                @user.UserName ile sohbet et
            </a>
        </li>
    }
</ul>
